{
    "env": {
        "es2021": true
    },
    "extends": [
        "standard",
        "plugin:cypress/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "comma-dangle": "off"
    },
    "plugins": ["cypress"],
    "globals": {
      "test": "readonly",
      "expect": "readonly",
      "it": "readonly",
      "cy": "readonly"

    }
}
